@{
    Layout = "_Layout";
}

@RenderBody()

@section scripts
{
<script src="~/js/docs.min.js" .dev-src="~/js/docs.js"></script>
    @await RenderSectionAsync("scripts", required: false)
}

@section header
{
<link rel="stylesheet" href="~/css/docs.css" />
    @await RenderSectionAsync("header", required: false)
}
